\NewDocumentCommand { \lcrollcall } { s +m } {
% 1=namspace,
% 2=seq name,
% 3=data

    \IfBooleanTF {#1}
    { \bool_gset_true:N \g_fc_rollcall_bool  }
    { \bool_gset_false:N \g_fc_rollcall_bool  }


    \mfsloadaseq [lc] { rollcall } { #2 }
    
     \int_gset:Nn
       \g_fc_totalitems_int
       {
       \seq_count:c
       { g_fc_rwe
       \tl_use:N
         \g_fc_namespace_tl
       rollcall
        _seq }
        }

     \int_gset:Nn
      \g_fc_curritem_int
      { 1 }


   \seq_map_function:cN
     { g_fc_rwe
       \tl_use:N
         \g_fc_namespace_tl
       rollcall
        _seq }
     \ic_funcrollcall:n
%     \adddot

}






