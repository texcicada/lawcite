 % 1=seq
   \seq_set_split:Nnn
     \l_tmpa_seq
     { ; }
     { #1 }
   \tl_set:Nx
     \l_tmpa_tl % bibentry key
     {
      \seq_item:Nn
        \l_tmpa_seq
        {1}
     }
   \tl_set:Nx
     \l_tmpb_tl % action on next decision
     {
      \seq_item:Nn
        \l_tmpa_seq
        {2}
     }

% first item
   \int_compare:nNnTF
     { \g_fc_curritem_int } = { 1 }
     {
      \lcinline % full case name\ref
         { \tl_use:N \l_tmpa_tl }
     }
     {
      \lawciteref %caseref only
         { \tl_use:N \l_tmpa_tl }
     }

%last item
   \int_compare:nNnTF
     { \g_fc_curritem_int } { = } { \g_fc_totalitems_int }
     {   } % no punct sep: delim
     { , \tex_space:D  } % delim comma space
 
     \tl_use:N \l_tmpb_tl % court action (empty on last decision)

%last item
   \int_compare:nNnTF
     { \g_fc_curritem_int } { = } { \g_fc_totalitems_int }
     {   } % no delim
     { \tex_space:D  } % delim space
      
