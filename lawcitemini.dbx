\NewDocumentCommand { \mfsloadaseq } { o m +m } { % 1=seq name, 2=data

    \IfNoValueTF { #1 }
      { \tl_clear:N \g_fc_namespace_tl }
      { \tl_gset:Nn \g_fc_namespace_tl { #1 } }


 \cs_if_free:cT
   { g_fc_rwe \g_fc_namespace_tl #2 _seq }
